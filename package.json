    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^10.1.1",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "playwright": "^1.57.0",
    "postcss": "^8.4.32",
