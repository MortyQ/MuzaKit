{
  "name": "muzakit",
  "version": "1.0.0",
  "description": "MuzaKit - Where inspiration meets development. A modern Vue 3 starter with glassmorphism design, TypeScript, Tailwind CSS, ESLint",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --fix",
    "lint:check": "eslint ./src",
    "format": "prettier --write .",
    "prepare": "husky",
    "deploy:preview": "pnpm build && pnpm preview",
    "deploy:check": "bash scripts/check-deploy-ready.sh",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:preview": "pnpm build-storybook && npx http-server storybook-static -p 6007",
    "deploy:storybook": "pnpm build-storybook"
  },
  "dependencies": {
    "@tanstack/vue-virtual": "^3.13.12",
