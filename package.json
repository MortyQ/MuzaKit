    "@vueuse/core": "^13.6.0",
    "axios": "^1.12.2",
    "dompurify": "^3.3.0",
    "echarts": "^6.0.0",
    "keyv-browser": "^0.1.1",
    "lodash.debounce": "^4.0.8",
    "luxon": "^3.7.2",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.25",
    "vue-echarts": "^8.0.1",
    "vue-markdown-render": "^2.3.0",
    "vue-multiselect": "^3.4.0",
    "vue-router": "^4.5.0",
    "vue-sonner": "^2.0.9"
